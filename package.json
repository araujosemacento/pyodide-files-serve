    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-devtools-json": "^0.2.1"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "svelte-i18n": "^4.0.1"
